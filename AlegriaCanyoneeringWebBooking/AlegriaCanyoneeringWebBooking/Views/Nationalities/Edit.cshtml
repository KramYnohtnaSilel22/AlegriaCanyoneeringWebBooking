@model AlegriaCanyoneeringWebBooking.Models.Nationality

@{
    ViewData["Title"] = "Edit Nationality";
}

<h1>Edit Nationality</h1>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    <div class="mb-3">
        <label asp-for="NatName" class="form-label"></label>
        <input asp-for="NatName" class="form-control" />
        <span asp-validation-for="NatName" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
